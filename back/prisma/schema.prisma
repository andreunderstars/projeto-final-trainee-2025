generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model album {
  id Int @id @default(autoincrement())
  title String
  artist String
  gender String
  releaseYear Int
  imageUrl String?

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime? @updatedAt @map("updated_at")

  @@map("albums")
}
